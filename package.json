{
  "name": "rook-website",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "pnpm run check && vinxi build",
    "start": "vinxi start",
    "check": "run-p check:* -c",
    "check:prettier": "prettier . --check",
    "check:lint": "eslint . --ext .cjs,.js,.jsx,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
    "check:types": "tsc --tsBuildInfoFile .tsbuildinfo --incremental",
    "check:deps": "pnpm install --frozen-lockfile --prefer-offline",
    "check:knip": "knip",
    "fix": "run-p fix:*",
    "fix:prettier": "prettier . --write",
    "fix:knip": "knip --fix --allow-remove-files"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.0",
    "@react-three/fiber": "^9.1.1",
    "@tanstack/react-router": "^1.114.29",
    "@tanstack/react-start": "^1.114.31",
    "@types/three": "^0.175.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "lucide-react": "^0.488.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-medium-image-zoom": "^5.2.14",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.175.0",
    "vinxi": "0.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^22.13.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^16.0.0",
    "knip": "^5.60.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
